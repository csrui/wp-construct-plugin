{
    "name": "csrui/wp-construct-plugin",
    "description": "Base library with plugin functionality to be extended on WordPress plugins",
    "type": "library",
    "license": "MIT",
    "keywords": ["wp", "wordpress"],
    "authors": [
        {
            "name": "Gulbenkian",
            "email": "devdigital@csrui.pt"
        }
    ],
    "repositories": [
      {
        "type": "vcs",
        "url": "git@github.com:nlemoine/wp-cli-fixtures.git"
      }
    ],
    "require": {
        "wp-cli/wp-cli": "2.0.1"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
        "hellonico/wp-cli-fixtures": "dev-master",
        "phpunit/phpunit": "4.0.*",
        "squizlabs/php_codesniffer": "^3.2",
        "wp-coding-standards/wpcs": "^0.14.0"
    },
    "autoload": {
        "psr-4": {
            "csrui\\WPConstruct\\Plugin\\": "lib"
        },
        "files": [ "cli.php" ]
    },
    "config": {
      "sort-packages": true
    }
}
